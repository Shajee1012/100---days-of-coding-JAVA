import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        System.out.print("Enter the number:");
        int n= sc.nextInt();
        if((n/2)*2==n)
        System.out.print(+n+ " is Even");
        else
        System.out.print(+n+ " is odd");
    }
}
